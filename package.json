    "@craco/craco": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "faker": "^5.5.3",
    "html-react-parser": "^1.2.6",
    "if-env": "^1.0.4",
    "react-quill": "^1.3.5",
    "react-sketch": "^0.5.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
